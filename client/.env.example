# Client Configuration
REACT_APP_API_URL=https://localhost:3001
REACT_APP_WS_URL=wss://localhost:3001
REACT_APP_ENCRYPTION_VERSION=1.0.0
REACT_APP_MAX_MESSAGE_SIZE=10000
REACT_APP_SESSION_TIMEOUT=3600000

# Security Headers (Meta tags)
REACT_APP_CSP_DEFAULT="'self'"
REACT_APP_CSP_SCRIPT="'self' 'unsafe-inline' 'unsafe-eval'"
REACT_APP_CSP_STYLE="'self' 'unsafe-inline'"

# Feature Flags
REACT_APP_ENABLE_DEBUG=false
REACT_APP_ENABLE_PWA=false
REACT_APP_ENABLE_OFFLINE=false